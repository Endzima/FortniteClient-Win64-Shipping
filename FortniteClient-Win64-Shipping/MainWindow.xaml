<Window x:Class="FortniteClient_Win64_Shipping.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FortniteClient_Win64_Shipping"
        mc:Ignorable="d"
        Title="Fortnite" Height="750" Width="1280" WindowStartupLocation="CenterScreen" WindowState="Normal" WindowStyle="None">
    <Grid>
        <Image Source="pack://application:,,,/Content/UI/Loading/ATLAS_LoadingScreen.png" Stretch="UniformToFill" RenderOptions.BitmapScalingMode="HighQuality"/>
        <Frame Name="Login" NavigationUIVisibility="Hidden"/>
        <Frame Name="FG" NavigationUIVisibility="Hidden"/>
        <Frame Name="Overlay" NavigationUIVisibility="Hidden" Height="226" Width="282" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,56,0,0"/>
        <Button Width="41" Height="36" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="20" Click="SettingsMenu_Click" Style="{StaticResource uibtn1}"/>
        <Frame Name="Settings" NavigationUIVisibility="Hidden" Visibility="Visible"/>
    </Grid>
</Window>
